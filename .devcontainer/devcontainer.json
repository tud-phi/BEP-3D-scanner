{
  "name": "Python 3.11 Dev",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "extensions": [
    "ms-python.python",
    "GitHub.remotehub"
  ],
  "settings": {
    "python.defaultInterpreterPath": "/usr/local/bin/python"
  },
  "postCreateCommand": "pip install --upgrade pip",
  "remoteUser": "root"
}
